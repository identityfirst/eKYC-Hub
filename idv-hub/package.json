{
  "name": "idv-hub",
  "version": "1.0.0",
  "description": "Verifiable Claims Hub",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "mock": "node mock/mockapp.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon   server/index.ts ",
    "dev:debug": "nodemon --exec \"node --inspect-brk=0.0.0.0:9229 --require ts-node/register server/index.ts\"",
    "lint": "eslint -c .eslintrc.js '{server, test}/**/*.{js,ts,tsx}' --quiet",
    "lint:fix": "eslint -c .eslintrc.js '{server, test}/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dot-object": "^2.1.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-openapi-validator": "^3.12.9",
    "express-session": "^1.17.1",
    "json-query": "^2.2.2",
    "jsonpath": "^1.0.2",
    "jsonschema": "^1.2.6",
    "jwt-decode": "^2.2.0",
    "mongodb": "^3.5.8",
    "openid-client": "^3.15.3",
    "pino": "^6.2.1",
    "string-similarity": "^4.0.4",
    "ts-custom-error": "^3.2.0",
    "uuid4": "^2.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/mongodb": "^3.5.20",
    "@types/pino": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "chai": "^4.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.0.0",
    "prettier": "^2.0.5",
    "shelljs": "^0.8.4",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.2"
  },
  "author": "Michal Kepkowski (mkepkowski@identityfirst.tech)"
